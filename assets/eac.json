{
  "Applications": {
    "alfresco-adf-default": {
      "Application": {
        "Description": "Alfresco ADF Default",
        "Name": "Alfresco ADF Default"
      },
      "LookupConfig": {
        "PathRegex": "/dashboard.*"
      },
      "LowCodeUnit": {
        "Package": "@fathym-hyland/alfresco-default",
        "Version": "latest",
        "Type": "NPM"
      },
      "Processor": {
        "BaseHref": "/dashboard/",
        "DefaultFile": "index.html",
        "Type": "DFS"
      }
    },
    "hyland-flow-storybook": {
      "Application": {
        "Description": "Hyland Flow Storybook",
        "Name": "Hyland Flow Storybook"
      },
      "LookupConfig": {
        "PathRegex": "/qa/storybooks/hyland-flow.*"
      },
      "LowCodeUnit": {
        "Package": "@fathym-hyland/hyland-flow-storybook",
        "Version": "latest",
        "Type": "NPM"
      },
      "Processor": {
        "BaseHref": "/qa/storybooks/hyland-flow/",
        "DefaultFile": "index.html",
        "Type": "DFS"
      }
    }
  },
  "Environments": {
    "$#.PrimaryEnvironment": {
      "Clouds": {
        "{{AZURE_CLOUD_LOOKUP}}": {
          "ResourceGroups": {
            "{{RESOURCE_NAME}}": {
              "Name": "{{RESOURCE_NAME}}",
              "Description": "{{RESOURCE_NAME}}"
            }
          }
        }
      }
    }
  }
}
